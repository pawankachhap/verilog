#! c:/iverilog-x64/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000010f6080 .scope module, "testing" "testing" 2 4;
 .timescale -9 -11;
v00000000010c2d20_0 .var "a", 0 0;
v0000000001148d50_0 .var "b", 0 0;
    .scope S_00000000010f6080;
T_0 ;
    %vpi_call 2 8 "$monitor", "[%3d]  a = %d  b = %d \012", $time, v00000000010c2d20_0, v0000000001148d50_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000010c2d20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000001148d50_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000010c2d20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000001148d50_0, 0, 1;
    %delay 100, 0;
    %load/vec4 v0000000001148d50_0;
    %assign/vec4 v00000000010c2d20_0, 0;
    %load/vec4 v00000000010c2d20_0;
    %assign/vec4 v0000000001148d50_0, 0;
    %delay 200, 0;
    %load/vec4 v0000000001148d50_0;
    %pad/u 2;
    %addi 1, 0, 2;
    %pad/u 1;
    %assign/vec4 v00000000010c2d20_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0000000001148d50_0, 500;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0000000001148d50_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0000000001148d50_0, 200;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0000000001148d50_0, 1000;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000001148d50_0, 0, 1;
    %delay 2000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "timing.v";
